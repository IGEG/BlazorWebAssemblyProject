@page "/"
@page "/{categoryUrl}"
@inject IDataCategory DataCategory
@inject IDataProduct DataProduct
@inject IDataState DataState

<PageTitle>Дистилляторы растворителей UltraClean</PageTitle>

<FirstPage/>
<Products/>

@code{
    [Parameter]
    public string CategoryUrl { get; set; }

    private Category category = null;

    protected override async Task OnParametersSetAsync()
    {
        await DataProduct.LoadProduct(CategoryUrl);
        await DataState.AddVisit();
        await DataState.GetVisits();

    }  
  
}